{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "prefer-const": "warn",
    "no-var": "error",
    "object-shorthand": "warn",
    "prefer-template": "warn"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
